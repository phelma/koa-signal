{
  "levels": {
    "success": {
      "badge": "‚úÖÔ∏è",
      "label": "success",
      "labelColor": [ "green" ],
      "format": [ "level", "id", "message" ]
    },

    "start": {
      "badge": "üöÄ",
      "labelColor": ["yellow"],
      "format": [ "level", "message" ]
    },

    "end": {
      "badge": "üõë",
      "labelColor": ["red"],
      "format": [ "level", "message" ]
    },

    "info": {
      "badge": "üí¨",
      "labelColor": ["blue"],
      "format": [ "level", "id", "file-name", "message" ]
    },

    "warn": {
      "badge": "‚ö†Ô∏è",
      "label": "warning",
      "labelColor": ["yellow"],
      "format": [ "level", "id", "file-name", "message" ]
    },

    "trace": {
      "badge": "‚ö°Ô∏è",
      "labelColor": ["blueBright"],
      "format": [ "level", "id", "file-name", "time-diff-init",  "time-diff-scope", "just:-", "scope", "message" ]
    },

    "error": {
      "badge": "üî•",
      "labelColor": ["red"],
      "format": [ "level", "id", "file-name", "message", "just:-", "error" ]
    },

    "access": {
      "format": [ "just:üö¶ ", "access", "id", "access-log" ]
    },

    "summary": {
      "badge": "üû∫",
      "label": "summary",
      "labelColor": "bold",
      "format": [ "level", "summary" ]
    }
  },

  "components": {
    "id": {
      "color": "grey",
      "displayShortId": 8,
      "displayFormat": "[%s]"
    },

    "file-name": {
      "color": "green",
      "displayFileName": false,
      "displayFormat": "@%s"
    },

    "error": {
      "color": "dim",
      "displayStackTrace": true,
      "displayFullStackTrace": false
    },

    "message": {
      "properties": [ "msg", "message", "text" ]
    },

    "level": {
      "minimumWidth": 20,
      "alignment": "left",
      "underline": true,
      "uppercase": false,
      "displayFormat": "%s ‚îÇ"
    },

    "scope": {
      "color": [ "dim" ],
      "alignment": "left",
      "displayFormat": "[%s]",
      "uppercase": false,
      "underline": false,
      "italic": false
    },

    "time-diff-init": {
      "defaultReturn": "0ms",
      "color": [ "magenta" ],
      "minimumWidth": 10,
      "alignment": "right"
    },

    "time-diff-scope": {
      "color": [ "blueBright" ],
      "defaultReturn": "0ms",
      "minimumWidth": 8,
      "alignment": "right"
    },

    "access": {
      "minimumWidth": 7,
      "displayFormat": "%s ‚îú",
      "displayMethod": false,
      "displayStatus": true,
      "color": [ ],
      "statusMap": {
        "200-300": { "color": ["green"], "badge":  "‚úî" },
        "300-400": { "color": ["blue"], "badge": "‚Üê" },
        "400-500": { "badge": "‚ö†" },
        "500-1000": { "color": ["red"], "badge": "‚úñ" }
      }
    },

    "access-log": {
      "format": ":method :host :path :statusCode :length - :responseTime_ms - :statusMessage"
    },

    "json": {
      "humanReadable": false,
      "properties": [ "id", "errors" ],
      "defaultProperties": [ "req", "res" ]
    }

  }
}
